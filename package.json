      "require": "./dist/*.js"
    }
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "eslint": "^9.19.0",
    "jest": "^29.0.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "string",
    "type-checking",
    "validation",
    "javascript",
    "utility"
  ]
